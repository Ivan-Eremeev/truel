block content

	.page

		include blocks/header.pug
		
		.content
			
			// pagination
			.pagination
				.wrapper
					ul.pagination__list 
						li 
							a(href="#") Главная
						li 
							a(href="#") Услуги
						li 
							span Обработка
			//	/pagination

			// titleblock
			.titleblock
				.wrapper
					h1 Обработка грузов
					h4 Работаем больше, чтобы вы платили меньше — объединим поставки из разных стран, отсортируем и отправим их сборным грузом.
			//	/titleblock

			// accoproc
			.accoproc
				.wrapper 
					.accoproc__container
						.line.line--1.mobile-hide
						.line.line--2
						.line.line--3.mobile-hide
						.accoproc__accordion
							.row 
								.accoproc__col
									.accoproc__img 
										img(src="img/pack.png", alt="" data-img="#trigger_1").active
										img(src="img/image-1.jpg", alt="" data-img="#trigger_2")
										img(src="img/image-2.jpg", alt="" data-img="#trigger_3")
										img(src="img/pack.png", alt="" data-img="#trigger_4")
										img(src="img/pack.png", alt="" data-img="#trigger_5")
